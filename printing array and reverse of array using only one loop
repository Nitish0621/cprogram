#include <stdio.h>

int main() {
   int arr[5],i,j=4;
   printf("Enter the elements\n");
   for(i=0;i<5;i++)
      {
          scanf("%d",&arr[i]);
      }
    for (i=0; j >= 0; i++) 
    {
       if(i<5)
       {
           printf("%d,",arr[i]);
           continue;
       }
       if(i==5)
         {
             printf("\n");
         }
         printf("%d,",arr[j]);
         j--;
    }
    return 0;
}
