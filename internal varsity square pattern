
#include <stdio.h>

int main() {
    int i,j,k=3,l=0;
    for(i=1;i<=4;i++)
        {
            for(j=1;j<=3;j++)
                {
                    if(i==1||i==4||j==1||j==3)
                       printf("%d",k);
                    else
                    printf("%d",l);
                }
                printf("\n");
                l++;
        }

    return 0;
}
