#include <stdio.h>

int main() {
    int arr[5],i,j,temp;
    printf("Enter the Elements\n");
    for(i=0;i<5;i++)
      scanf("%d",&arr[i]);
    
    for(i=0;i<5;i++)
        {   
            for(j=0;j<5;j++)
              {
                 if(arr[i]>arr[i+1])
                    { 
                      temp=arr[i];
                      arr[i]=arr[i+1];
                      arr[i+1]=temp;
                    }
              }       
             
            printf("%d ",arr[i]);
        }

    return 0;
}
