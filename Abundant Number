#include <stdio.h>
int main()
{
    int x,i,y=0;
    printf("Enter the number\n");
    scanf("%d",&x);
    for(i=1;i<x;i++)
      {
        if(x%i==0)
           y=y+i;
      }
        if(y>x)
          printf("%d is a Abundant Number",x);
        else
          printf("%d is not a Abundant Number",x);
    return 0;
}
