#include<stdio.h>
int main()
{
 int i,j,k=0,rows;
 printf("Enter the number of rows\n");
 scanf("%d",&rows);
 for(i=1;i<=rows;i++)
    {
        for(j=1;j<=rows;j++)
            {   k++;
                if(j==(rows+1)-1)
                  {
                    printf("%d",k);
                     
                  }    
                else
                  {
                      printf("%d*",k);
                  }
            }
            printf("\n");
    }   
 return 0;
}
