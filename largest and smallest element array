#include <stdio.h>

int main()
{
   int max,min,arr[5],i;
   printf("Enter the element of array\n");
   for(i=0;i<5;i++)
      {
        scanf("%d",&arr[i]);
      }
      max=min=arr[0];
    for(i=0;i<5;i++)
      {
          if(max<arr[i])
            {
                max=arr[i];
            }
            if(min>arr[i])
            {
                min=arr[i];
            }
      }
   
printf("largest element is %d ,smallest element is %d ",max,min);
    return 0;
}
