#include<stdio.h>
int main()
{
 int i,j,k=2;
 for(i=1;i<=7;i++)
    {
        for(j=1;j<=4;j++)
            {   
                if(i>=j&&i<=8-j)
                  {
                    i<=4?k++:k--;
                    printf("%d",k);
                      
                  }    
                
            }
            printf("\n");
    }   
 return 0;
}
